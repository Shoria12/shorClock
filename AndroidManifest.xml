<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.sec.android.widgetapp.digitalclock">
    <application android:icon="@drawable/preview" android:label="@string/app_name">
        <receiver android:label="@string/app_name" android:name=".DigitalClockWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
                <action android:name="sec.android.intent.action.HOME_RESUME"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/digitalclock_appwidget_info"/>
        </receiver>
        <service android:name=".DigitalClockService">
            <intent-filter>
                <action android:name="digitalclock.action.startservice"/>
                <action android:name="digitalclock.action.stopservice"/>
            </intent-filter>
        </service>
    </application>
</manifest>